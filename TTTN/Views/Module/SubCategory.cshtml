@model IEnumerable<TTTN.Models.C_category>
@{ var subcat = ViewBag.subcategory; }

<li class="dropdown" style="width:272px !important;">
    <a href="@subcat.category_slug" class="dropdown-toggle" data-toggle="dropdown">@subcat.category_name<span class="caret"></span></a>
    <div class="dropdown-menu mega-dropdown-menu w3ls_vegetables_menu">
        <div class="w3ls_vegetables list-group">
            <ul>
                @foreach (var item in Model)
                {
                    <li class="list-group-item" style="border:none;"><a href="@item.category_slug">@item.category_name</a></li>
                }

            </ul>
        </div>
    </div>
</li>